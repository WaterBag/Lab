    {
      "type": "num",
      "desc": "",
      "required": false,
      "name": "random_state",
      "label": "随机数种子",
      "default": null
    }
  ],
  "inputConfig": [
    {
      "name": "in_1",
      "desc": "DataFrame数据",
      "required": true,
      "label": "输入DataFrame"
    }
  ],
  "outputConfig": [
    {
      "desc": "训练后的随机森林模型",
      "name": "rf",
      "label": "随机森林模型"
    }
