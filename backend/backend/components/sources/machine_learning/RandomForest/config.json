{
  "name": "随机森林",
  "category": "machine_learning",
  "genre": "common",
  "paramsConfig": [
    {
      "type": "str",
      "desc": "数据集中作为X的列",
      "required": true,
      "name": "col_x",
      "label": "特征列",
      "default": null
    },
    {
      "type": "str",
      "desc": "数据集中作为Y的列",
      "required": true,
      "name": "col_y",
      "label": "选择预测列",
      "default": null
    },
    {
      "type": "opt",
      "desc": "类型",
      "required": true,
      "name": "genre",
      "label": "分析类型",
      "default": "Classifier",
      "options": [
        {
          "label": "分类",
          "value": "Classifier"
        },
        {
          "label": "回归",
          "value": "Regressor"
        }
      ]
    },
    {
      "type": "num",
      "desc": "",
      "required": false,
      "name": "n_estimators",
      "label": "迭代次数",
      "default": 100
    },
    {
      "type": "opt",
      "desc": "最大特征数",
      "required": true,
      "name": "max_features",
      "label": "最大特征数",
      "default": "auto",
      "options": [
        {
          "label": "auto",
          "value": "'auto'"
        },
        {
          "label": "sqrt",
          "value": "'sqrt'"
        },
        {
          "label": "log2",
          "value": "'log2'"
        },
        {
          "label": "None",
          "value": "None"
        }
      ]
    },
    {
      "type": "num",
      "desc": "",
      "required": false,
      "name": "max_depth",
      "label": "树最大深度",
      "default": null
